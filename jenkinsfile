pipeline {
    agent any

    stages {
        stage('Permisos') {
            steps {
                 sh 'chmod 777 ./mvnw'
            }
            
        stage('Build') {
            steps {
                 sh './mvnw clean -e'
            }
        }

        stage('test') {
            steps {
                sh './mvnw clean compile -e'
            }
        }

        stage('test2') {
            steps {
                 sh './mvnw clean test -e'
            }
        }
    }
}
